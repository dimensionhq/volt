#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/varunp/Documents/volt/pnpm_test/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/home/varunp/Documents/volt/pnpm_test/node_modules/.pnpm/mime@1.6.0/node_modules:/home/varunp/Documents/volt/pnpm_test/node_modules/.pnpm/node_modules:/home/varunp/Documents/volt/pnpm_test/node_modules:/home/varunp/Documents/volt/node_modules:/home/varunp/Documents/node_modules:/home/varunp/node_modules:/home/node_modules:/node_modules:/home/varunp/Documents/volt/pnpm_test/node_modules/.pnpm/node_modules/mime/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
